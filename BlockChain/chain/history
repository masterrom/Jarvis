var securitycontractContract = web3.eth.contract([{"inputs":[{"internalType":"uint8","name":"_numProposals","type":"uint8"}],"payable":false,"stateMutability":"nonpayable","type":"constructor"},{"constant":false,"inputs":[{"internalType":"string","name":"newData","type":"string"}],"name":"newMessage","outputs":[],"payable":false,"stateMutability":"nonpayable","type":"function"}]);
var securitycontract = securitycontractContract.new(
   _numProposals,
   {
     from: web3.eth.accounts[0], 
     data: '', 
     gas: '4700000'
   }, function (e, contract){
    console.log(e, contract);
    if (typeof contract.address !== 'undefined') {
         console.log('Contract mined! address: ' + contract.address + ' transactionHash: ' + contract.transactionHash);
    }
 })
